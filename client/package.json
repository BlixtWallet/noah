{
  "name": "noah",
  "version": "0.0.5",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "android:clean": "rm -rf android/.gradle android/app/build android/build android/.cxx android/app/.cxx android/app/.idea && cd android && ./gradlew clean",
    "ios:clean": "rm -rf ios/Pods ios/Podfile.lock ios/build/",
    "clean:all": "bun android:clean && bun ios:clean",
    "android:mainnet:debug": "expo run:android --variant mainnetDebug --app-id=com.noahwallet",
    "android:mainnet:release": "expo run:android --variant mainnetRelease --app-id=com.noahwallet",
    "android:signet:debug": "expo run:android --variant signetDebug --app-id=com.noahwallet.signet",
    "android:signet:release": "expo run:android --variant signetRelease --app-id=com.noahwallet.signet",
    "android:regtest:debug": "expo run:android --variant regtestDebug --app-id=com.noahwallet.regtest",
    "android:regtest:release": "expo run:android --variant regtestRelease --app-id=com.noahwallet.regtest",
    "ios:mainnet:debug": "expo run:ios --scheme Noah --configuration Debug",
    "ios:mainnet:release": "expo run:ios --scheme Noah --configuration Release",
    "ios:signet:debug": "expo run:ios --scheme Noah-Signet --configuration Debug",
    "ios:signet:release": "expo run:ios --scheme Noah-Signet --configuration Release",
    "ios:regtest:debug": "expo run:ios --scheme Noah-Regtest --configuration Debug",
    "ios:regtest:release": "expo run:ios --scheme Noah-Regtest --configuration Release",
    "web": "expo start --web",
    "prepare": "husky",
    "build:android:signet:release": "cd android && ./gradlew assembleSignetRelease",
    "build:android:regtest:release": "cd android && ./gradlew assembleRegtestRelease",
    "build:android:mainnet:release": "cd android && ./gradlew assembleMainnetRelease",
    "build:android:signet:debug": "cd android && ./gradlew assembleSignetDebug",
    "build:android:regtest:debug": "cd android && ./gradlew assembleRegtestDebug",
    "build:android:mainnet:debug": "cd android && ./gradlew assembleMainnetDebug",
    "build:android:ci": "cd android && ./gradlew assembleSignetRelease -PreactNativeArchitectures=arm64-v8a",
    "ios:prebuild": "cd ios && pod install --repo-update",
    "build:ios:mainnet:debug": "xcodebuild build -workspace ios/noah.xcworkspace -scheme Noah -configuration Debug -sdk iphonesimulator -quiet ARCHS=arm64",
    "build:ios:mainnet:release": "xcodebuild build -workspace ios/noah.xcworkspace -scheme Noah -configuration Release -sdk iphonesimulator -quiet ARCHS=arm64",
    "build:ios:signet:debug": "xcodebuild build -workspace ios/noah.xcworkspace -scheme Noah-Signet -configuration Debug -sdk iphonesimulator -quiet ARCHS=arm64",
    "build:ios:signet:release": "xcodebuild build -workspace ios/noah.xcworkspace -scheme Noah-Signet -configuration Release -sdk iphonesimulator -quiet ARCHS=arm64",
    "build:ios:regtest:debug": "xcodebuild build -workspace ios/noah.xcworkspace -scheme Noah-Regtest -configuration Debug -sdk iphonesimulator -quiet ARCHS=arm64",
    "build:ios:regtest:release": "xcodebuild build -workspace ios/noah.xcworkspace -scheme Noah-Regtest -configuration Release -sdk iphonesimulator -quiet ARCHS=arm64",
    "build:ios:ci": "xcrun xcodebuild -scheme 'Noah-Regtest' -workspace 'ios/noah.xcworkspace' -configuration Release -sdk 'iphonesimulator' -destination 'generic/platform=iOS Simulator' -derivedDataPath ios/build -quiet ARCHS=arm64",
    "build:ios:signet:device": "xcrun xcodebuild -scheme 'Noah-Signet' -workspace 'ios/noah.xcworkspace' -configuration Release -sdk iphoneos -derivedDataPath ios/build -allowProvisioningUpdates build -quiet ARCHS=arm64",
    "nitrogen": "bun --cwd nitromodules/noah-tools nitrogen",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "typecheck": "tsc --noEmit",
    "check": "bun run lint && bun run typecheck",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "maestro:test": "maestro test --debug-output maestro-debug-output .maestro"
  },
  "dependencies": {
    "@bottom-tabs/react-navigation": "^1.1.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/slider": "5.1.2",
    "@react-native-vector-icons/get-image": "^12.3.0",
    "@react-native-vector-icons/ionicons": "^12.3.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "@rn-primitives/accordion": "^1.2.0",
    "@rn-primitives/alert-dialog": "^1.2.0",
